---
import "../styles/Beta.css";
const { title } = Astro.props;
import "@fontsource/rubik/300.css";
import "@fontsource/rubik/400.css";
import "@fontsource/rubik/500.css";
import "@fontsource/rubik/600.css";
import "@fontsource/cinzel-decorative/400.css";
---

<html lang="en">
  <head>
    <meta charset="utf-8" />
    <link rel="icon" type="image/svg+xml" href="/Favicon.png" />
    <meta name="viewport" content="width=device-width" />
    <meta
      name="description"
      content="Welcome to Chattahoochee High School Beta Club's Homepage! Beta Club serves as a platform through which students can volunteer in their community."
    />
    <meta
      name="keywords"
      content="Hoochbeta, hoochbeta,Hooch Beta, Chattahoochee Beta Club, High School Beta Club, Chattahoochee Beta, Chattahoochee High School Beta Club"
    />
    <meta
      name="google-site-verification"
      content="48CeVyJtaEiwuLCf0eFrX-r6_Z6Eoy7PGs9BTmqfovQ"
    />
    <meta name="generator" content={Astro.generator} />
    <title>{title}</title>
    <script
      src="https://kit.fontawesome.com/2aa51cc09e.js"
      crossorigin="anonymous"></script>
  </head>
  <body>
    <slot> Default</slot>
  </body>
